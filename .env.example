# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google OAuth (for Google Sign-In)
# Web Client ID from Google Cloud Console (required for both iOS and Android)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_web_client_id
# iOS Client ID from Google Cloud Console (optional, for iOS-specific configuration)
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id

# Apple OAuth (for Apple Sign-In)
EXPO_PUBLIC_APPLE_OAUTH_CLIENT_ID=your_apple_oauth_client_id

# Listen Notes API
EXPO_PUBLIC_LISTEN_API_KEY=your_listen_notes_api_key

# Stripe
EXPO_PUBLIC_STRIPE_PAYMENT_LINK=your_stripe_payment_link